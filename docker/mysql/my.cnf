[mysqld]
sql_mode = ERROR_FOR_DIVISION_BY_ZERO,NO_AUTO_CREATE_USER,NO_ENGINE_SUBSTITUTION
group_concat_max_len = 320000
max_allowed_packet = 128M
innodb_buffer_pool_size = 1G

[mysql]
default-character-set = utf8mb4

[mysqldump]
default-character-set = utf8mb4

[client]
default-character-set = utf8mb4